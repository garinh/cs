<arbvp>
  <program>
  <![CDATA[!!ARBvp1.0
    PARAM mvp[4]    = { state.matrix.mvp };
    ATTRIB iPos     = vertex.position;
    OUTPUT oPos     = result.position;

    DP4 oPos.x, mvp[0], iPos;
    DP4 oPos.y, mvp[1], iPos;
    DP4 oPos.z, mvp[2], iPos;
    DP4 oPos.w, mvp[3], iPos;

  END]]>
  </program>
</arbvp>
