            </params>
            <zfill />
        </meshobj>
        <portals>
            <portal name="north">
                <v x="-1" y="1" z="6" />
                <v x="0" y="1" z="6" />
                <v x="0" y="-1" z="6" />
                <v x="-1" y="-1" z="6" />
                <sector>large</sector>
                <viscull />
